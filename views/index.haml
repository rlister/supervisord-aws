%div.container
  %div.container.col-md-6.col-md-offset-3
    %ul.list-group
    - @groups.each do |name, regions|
      %li.list-group-item
        %span.lead
          %a{href: "/#{name}"}= name
        %span.pull-right
          - regions.each do |region|
            %span.label.label-default= region
